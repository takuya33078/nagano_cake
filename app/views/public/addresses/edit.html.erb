<div class="container">
  <div class="row">
     <div class="col-lg-8">
    <h10 style="width: 100px;background-color: #d3d3d3">配送先編集</h10>
    </div>
       <div class="col-md-8">
    <%= form_with model: @address, url:public_address_path do |f| %>
     <div class="address">
       <div class="form-group col-xs-12">
        <%= f.label :"郵便番号(ハイフンなし)" ,class: "col-xs-3 control-label" %>
        <%= f.text_field :postal_code, class: "col-xs-4 form-control-static postal_code" %>
        </div>

        <div class="form-group col-xs-12">
         <%= f.label :"住所" ,class: "col-xs-3 control-label" %>
         <%= f.text_field :address, class: "col-xs-7 form-control-static address" %>
        </div>

        <div class="form-group col-xs-12">
         <%= f.label :"宛名" ,class: "col-xs-3 control-label" %>
         <%= f.text_field :name, class: "col-xs-4 form-control-static name" %> <%= f.submit '変更内容を保存', class:"btn btn-success" %>
        </div>

     </div>
    <% end %>
   </div>
  </div>
</div>