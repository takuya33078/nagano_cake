<div class="row">
 <div class="col-xs-3"></div>
 <div class="col-md-10 col-md-offset-1">
 <h10 style="width: 100px;background-color: #d3d3d3">管理者ログイン</h10>
<br>
<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email, "メールアドレス"  %>    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div><br><br>

  <div class="field">
    <%= f.label :password, "パスワード"  %>              <%= f.password_field :password, autocomplete: "current-password" %>
  </div><br><br>


  <div class="actions">
    <%= f.submit "ログイン", class: "btn btn-primary btn-lg center-block" %>
  </div>
<% end %>

 </div>


</div>
